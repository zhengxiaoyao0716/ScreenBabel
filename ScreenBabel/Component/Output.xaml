<Window x:Class="ScreenBabel.Component.Output"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScreenBabel.Component"
        mc:Ignorable="d"
        Title="Output" Height="450" Width="600" Topmost="True" SnapsToDevicePixels="True">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="Raw" Margin="10,10,10,10" Grid.Row="0" TextWrapping="Wrap" IsReadOnly="True"/>
        <TextBox x:Name="Out" Margin="10,10,10,10" Grid.Row="1" TextWrapping="Wrap" IsReadOnly="True"/>

    </Grid>
</Window>
